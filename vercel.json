{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "api/v1/(.*)",
        "dest": "/",
        "methods":["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "headers":[
          {
            "Access-control-Allow-Origin": "*"
          }
        ]
      }
    ]
  }
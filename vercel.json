{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "api/v1/(.*)",
        "dest": "/",
        "methods":["GET", "POST", "PUT", "DELETE", "OPTIONS"]
      }
    ],
      "headers": [
        {
          "source": "/api/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "https://lms-frontend-iota-puce.vercel.app" }
          ]
        }
      ]
  }